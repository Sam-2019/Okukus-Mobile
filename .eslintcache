ll the form\");\r\n    }\r\n\r\n    addReview({\r\n      variables: {\r\n        user: String(uniqueID),\r\n        product: String(product),\r\n        rating: Number(starsSelected),\r\n        text: String(text),\r\n      },\r\n    });\r\n\r\n    if (reviewError) {\r\n      setMessage(reviewError);\r\n    }\r\n\r\n     close();\r\n  }\r\n\r\n  return (\r\n    <form>\r\n      <StarRating\r\n        starsSelected={starsSelected}\r\n        setStarsSelected={setStarsSelected}\r\n      />\r\n\r\n      <TextArea\r\n        class_name=\"text-input \"\r\n        placeholder=\"Leave a review\"\r\n        action={(e) => setText(e.target.value)}\r\n        value={text}\r\n      />\r\n\r\n      {message ? <Message class_name=\"message \" message={message} /> : null}\r\n\r\n      <Button class_name=\"primary\" name=\"Post\" action={submit} />\r\n\r\n      <Button class_name=\"secondary\" name=\"Cancel\" action={close} />\r\n    </form>\r\n  );\r\n};\r\n\r\nexport default AddReview;\r\n\r\nAddReview.propTypes = {\r\n  close: PropTypes.func,\r\n};\r\n","C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Components\\Social.js",[],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Review\\reviewData.js",[],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Cart\\cartData.js",["412","413","414"],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Components\\Star.js",[],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Components\\StarRating.js",["415"],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Components\\BinFill.js",[],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\Components\\Add.js",[],"C:\\Users\\Mvjvw\\Desktop\\Okukus Mobile\\src\\Pages\\User\\Profile\\Order History.js",["416","417"],"import React from \"react\";\r\nimport Back from \"../../Components/Back\";\r\nimport { useQuery } from \"@apollo/client\";\r\nimport { GET_ORDER } from \"../../graphQL functions\";\r\nimport { useData } from \"../../Context\";\r\n\r\nimport HistoryData f